{
  "name": "@containous/faency",
  "description": "Containous React component library",
  "version": "0.3.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://containous.github.io/faency/",
  "repository": "https://github.com/containous/faency",
  "bugs": {
    "url": "https://github.com/containous/faency/issues"
  },
  "keywords": [
    "containous",
    "faency",
    "react",
    "components",
    "component library",
    "design system"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9009 --ci",
    "build-storybook": "build-storybook",
    "build": "rm -rf dist && rollup -c",
    "watch": "rollup -cw",
    "prepublishOnly": "npm run build",
    "deploy-storybook": "build-storybook",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run format",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build && npm run test"
    }
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@styled-system/css": "5.1.4",
    "@styled-system/theme-get": "5.1.2",
    "@styled-system/variant": "^5.1.4",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "mdlz-prmtz": "^0.0.56",
    "polished": "^3.4.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-options": "5.2.8",
    "@storybook/addon-storysource": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@storybook/storybook-deployer": "^2.8.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-loader": "8.0.6",
    "csstype": "^2.6.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-extended": "^0.11.2",
    "jest-styled-components": "^6.3.4",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-eva-icons": "0.0.8",
    "rollup": "^1.27.12",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-url": "^3.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "3.7.3",
    "yalc": "^1.0.0-pre.32"
  },
  "author": "",
  "license": "Apache-2.0"
}
